{
  "hosting": {
    "public": "dist/labo4-tp/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/fonts/*.ttf",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/ttf"
          }
        ]
      },
      {
        "source": "**/fonts/*.otf",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/otf"
          }
        ]
      }
    ]
  }
}
